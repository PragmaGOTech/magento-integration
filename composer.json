{
    "name": "pragmagotech/magento2-module-pragmapay",
    "description": "PragmaGo Tech Magento2 Payment method integration",
    "type": "magento2-module",
    "version": "1.0.0",
    "require": {
        "php": "~8.1||~8.2||~8.3",
        "ext-pcre": "*",
        "magento/framework": "^103.0",
        "guzzlehttp/guzzle": "^7.9",
        "magento/module-checkout": "^100.4",
        "magento/module-payment": "^100.4",
        "magento/module-quote": "^101.2",
        "magento/module-sales": "^103.0",
        "magento/module-store": "^101.1",
        "magento/module-catalog": "^104.0",
        "magento/module-widget": "^101.2"
    },
    "license": [
        "proprietary"
    ],
    "autoload": {
        "files": [
            "src/PragmaPayAdminUi/registration.php",
            "src/PragmaPayCalculator/registration.php",
            "src/PragmaPayCore/registration.php",
            "src/PragmaPayFrontendUi/registration.php",
            "src/PragmaPayWebApi/registration.php"
        ],
        "psr-4": {
            "Pragma\\PragmaPayAdminUi\\": "src/PragmaPayAdminUi",
            "Pragma\\PragmaPayCalculator\\": "src/PragmaPayCalculator",
            "Pragma\\PragmaPayCore\\": "src/PragmaPayCore",
            "Pragma\\PragmaPayFrontendUi\\": "src/PragmaPayFrontendUi",
            "Pragma\\PragmaPayWebApi\\": "src/PragmaPayWebApi"
        }
    }
}
